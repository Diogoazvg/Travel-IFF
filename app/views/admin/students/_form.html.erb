<%= simple_form_for @admin_student, wrapper: :horizontal_form do |f| %>
  <%= f.error_notification %>
<div class="form-horizontal col-sm-offset-1 col-sm-7">
  <div class="panel panel-info">
  <div class="panel-heading ">Table_name</div>
    <div class="panel-body">
      <div class="form-inputs">
          <%= f.input :registration_file_name %>
          <%= f.input :registration_content_type %>
          <%= f.input :registration_file_size %>
          <%= f.input :registration_updated_at %>
          <%= f.association :person %>
      </div>

      <div class="form-actions">
          <%= f.button :submit, class: "btn btn-info" %>
      </div>
    </div>  
  </div>
</div>     
<% end %>
