<%= simple_form_for [:admin, @admin_student], wrapper: :horizontal_form do |f| %>
  <%= f.error_notification %>
<div class="form-horizontal col-sm-offset-1 col-sm-7">
  <div class="panel panel-info">
  <div class="panel-heading ">Table_name</div>
    <div class="panel-body">
      <div class="form-inputs">
          <%= f.input :registration %>
      </div>

      <div class="form-inputs col-sm-6">
        <%= f.simple_fields_for :person do |person| %>
          <%= person.input :name %>
          <%= person.input :phone %>
          <%= person.input :address %>
          <%= person.input :identity %>
          <%= person.input :cpf %>
          <%= person.input :birth %>
          <!--%= person.input :estado %-->   
        <% end %>
      </div>

      <div class="form-actions">
          <%= f.button :submit, class: "btn btn-info" %>
      </div>
    </div>  
  </div>
</div>     
<% end %>