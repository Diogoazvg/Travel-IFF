<div class='page-header'>
  <h1>
    <%= t('Listing Admin::BusCompanies') %> (<%= @count_admin_bus_companies %>)  <small><%= @admin_bus_company %></small>
    <div class='pull-right'>
      <%= link_to new_admin_bus_company_path(), class: 'btn btn-info' do %>
        <i class='glyphicon glyphicon-plus'></i> <%= t('New Admin::BusCompany') %>
      <% end %>
    </div>
  </h1>
</div>

<table class='table table-responsive table-hover table-stripped' id='admin_bus_companies'>
  <thead>
  <tr>
      <th><%=t("Cnpj")%></th>
      <th><%=t("Name")%></th>
    <th></th>
  </tr>

  </thead>

  <tbody>
    <% @admin_bus_companies.each do |admin_bus_company| %>
    <tr>
        <td><%= admin_bus_company.cnpj %></td>
        <td><%= admin_bus_company.name %></td>
      <td class='actions text-center'>
        <%= link_to t('Show'), admin_bus_company_path(admin_bus_company), class: 'btn btn-sm btn-info' %>
        <%= link_to t('Edit'), edit_admin_bus_company_path(admin_bus_company), class: 'btn btn-sm btn-warning' %>
        <%= link_to t('Destroy'), admin_bus_company_path(admin_bus_company), method: :delete, class: 'btn btn-sm btn-danger', data: { confirm: t('Are you sure?') } %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
<!--%%= paginate @admin_bus_companies %-->