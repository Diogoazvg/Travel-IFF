<div class='page-header'>
  <h1>
    <%= t('Admin::person Info') %>
    <div class='pull-right'>
      <%= link_to edit_admin_person_path(@admin_person), class: 'btn btn-info' do %>
        <i class='glyphicon glyphicon-pencil'></i> <%= t('Edit') %>
      <% end %>
      <%= link_to admin_people_path, class: 'btn btn-warning' do %>
        <i class='glyphicon glyphicon-backward'></i> <%= t('Back') %>
      <% end %>
    </div>
  </h1>
</div>

<div class='panel panel-default'>
  <div class='panel-body'>
    <p>
      <strong>Name</strong>
      <%= @admin_person.name %>
    </p>
    <p>
      <strong>Phone</strong>
      <%= @admin_person.phone %>
    </p>
    <p>
      <strong>Address</strong>
      <%= @admin_person.address %>
    </p>
    <p>
      <strong>Identity</strong>
      <%= @admin_person.identity %>
    </p>
    <p>
      <strong>Cpf</strong>
      <%= @admin_person.cpf %>
    </p>
    <p>
      <strong>Birth</strong>
      <%= @admin_person.birth %>
    </p>
  </div>
</div>
