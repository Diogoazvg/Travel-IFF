<%= simple_form_for @admin_person, wrapper: :horizontal_form do |f| %>
  <%= f.error_notification %>
<div class="form-horizontal col-sm-offset-1 col-sm-7">
  <div class="panel panel-info">
  <div class="panel-heading ">Table_name</div>
    <div class="panel-body">
      <div class="form-inputs">
          <%= f.input :name %>
          <%= f.input :phone %>
          <%= f.input :address %>
          <%= f.input :identity %>
          <%= f.input :cpf %>
          <%= f.input :birth %>
      </div>

      <div class="form-actions">
          <%= f.button :submit, class: "btn btn-info" %>
      </div>
    </div>  
  </div>
</div>     
<% end %>
